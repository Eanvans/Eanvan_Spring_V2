<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC  "-//mybatis.org//DTD mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Eanvan.mapper.ResourceMapper">

    <select id="getEanvanImgResourceByName" resultType="com.Eanvan.model.ImgResource">
        select * from tb_img_resource where name = #{name};
    </select>

    <select id="getEanvanContentResourceByName" resultType="com.Eanvan.model.Content">
        select * from tb_content_resource where name = #{name};
    </select>
</mapper>
